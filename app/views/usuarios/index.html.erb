<h1>Listing usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apepaterno</th>
      <th>Apematerno</th>
      <th>Distrito</th>
      <th>Fecnac</th>
      <th>Sexo</th>
      <th>Email</th>
      <th>Telfijo</th>
      <th>Telmovil</th>
      <th>Alias</th>
      <th>Saldo</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.nombre %></td>
        <td><%= usuario.apePaterno %></td>
        <td><%= usuario.apeMaterno %></td>
        <td><%= usuario.distrito_id %></td>
        <td><%= usuario.fecNac %></td>
        <td><%= usuario.sexo %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.telFijo %></td>
        <td><%= usuario.telMovil %></td>
        <td><%= usuario.alias %></td>
        <td><%= usuario.saldo %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
